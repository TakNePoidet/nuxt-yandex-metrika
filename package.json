{
  "name": "nuxt-yandex-metrika",
  "version": "1.3.0",
  "description": "Add Yandex Metrika to your Nuxt 3 application.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TakNePoidet/nuxt-yandex-metrika.git"
  },
  "homepage": "TakNePoidet/nuxt-yandex-metrika",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "TakNePoidet <https://taknepoidet.xyz/>",
    "url": "https://github.com/TakNePoidet"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "prettier": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "chalk": "^5.6.2",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@poidet/prettier": "^0.8.4",
    "@types/node": "^25.0.3",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nuxt": "^4.2.2",
    "prettier": "^3.7.4",
    "typescript": "5.9.3",
    "vue-tsc": "^3.2.1"
  },
  "lint-staged": {
    "**/*.(ts|vue)": [
      "eslint --fix",
      "git add"
    ],
    "**/*.*": [
      "prettier --write",
      "git add"
    ]
  },
  "packageManager": "pnpm@10.26.2"
}
